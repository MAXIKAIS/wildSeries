{% extends 'base.html.twig' %}
{% block title %}{{ category }}{% endblock %}
{% block body %}
    <h1>Les 3 dernières series de la catégorie : "{{ category }}"</h1>
{% for program in programs %}
    <h2>{{ program.id }}/ {{ program.title }}</h2>
    <p>{{ program.summary }}</p>
    {% endfor %}
{% endblock %}